    "description": "Allows users who have forgotten their password to unlock, retrieve, or reset it.",
    "keywords": [
        "forgot-password",
        "syehan",
        "laravel",
        "october",
        "octobercms",
        "otp",
        "email"
    ],
    "require": {
        "composer/installers": "~1.0",
        "spomky-labs/otphp": "^11.2"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}

